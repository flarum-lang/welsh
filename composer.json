{
	"name": "flarum-lang/welsh",
	"type": "flarum-extension",
	"description": "Welsh language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/welsh",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"cy-locale",
		"welsh-locale",
		"welsh"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/welsh/issues",
		"source": "https://github.com/flarum-lang/welsh"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Welsh",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "cy",
			"title": "Welsh"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
